{
  "name": "gis-3d-engine",
  "version": "1.0.0",
  "type": "module",
  "description": "Cesium 3D Tiles Viewer",
  "main": "dist/cesium-viewer.umd.js",
  "module": "dist/cesium-viewer.esm.js",
  "scripts": {
    "start": "live-server --host=localhost --port=8000 --open=viewer.html",
    "build": "rollup -c",
    "docs:api": "jsdoc -c jsdoc.json",
    "docs:start": "npm run docs:api && npm --prefix docs-site run start",
    "docs:build": "npm run docs:api && npm --prefix docs-site run build",
    "docs:serve": "npm --prefix docs-site run serve -- --build --port 8088 --host localhost"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "cesium": "^1.134.1",
    "geotiff": "^2.1.4-beta.1",
    "three": "^0.181.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "jsdoc": "^4.0.5",
    "live-server": "^1.2.2",
    "rollup": "^4.53.3"
  }
}
